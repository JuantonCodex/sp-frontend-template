//PlacesOfPayment - Online Payment
.c-PlacesOfPayment.c-PlacesOfPayment--onlinePayment.is-visible(data-nom="general")
  //- h1.c-PlacesOfPayment-title Selecciona tu banco
  .c-PlacesOfPayment-slideWrapper
    .c-PlacesOfPayment-listButtons
    .c-PlacesOfPayment-list
      each val in [1]
        .c-PlacesOfPayment-item
          .link-wrapper
            a.link.is-selected(href="#", data-nom-entidad="multibanco")
              img(src="img/shop-places/logo-mybank.png", alt="Mybank")
        
  //- Search component
  .c-PlacesOfPayment__myBankSearch
    include ../../c-mybank-search.jade